{
    "name": "daceej/waffle",
    "description": "A tool to prevent waffling!",
    "type": "project",
    "authors": [
        {
            "name": "Curtis Ogle",
            "email": "curtis.jams.ogle@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "^5.1",
        "symfony/yaml": "^5.1",
        "symfony/process": "^5.1",
        "ext-json": "*",
        "symfony/finder": "^5.1",
        "symfony/dependency-injection": "^5.2",
        "symfony/config": "^5.2",
        "consolidation/self-update": "^1.2"
    },
    "bin": [
        "bin/waffle",
        "bin/wfl"
    ],
    "autoload": {
        "psr-4": {
            "Waffle\\": "src"
        },
        "classmap": ["src"]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    }
}
